{
  "languageserver": {
    "cquery": {
      "command": "/Users/agrabauskas/bin/cquery/build/release/bin/cquery",
      "args": ["--log-file=/tmp/cq.log"],
      "filetypes": ["c", "cpp"],
      "rootPatterns": ["compile_flags.txt", "compile_commands.json", ".git/", ".hg/"],
      "initializationOptions": {
        "cacheDirectory": "/tmp/cquery"
      }
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true,
        "staticcheck": true,
        "watchChangedFiled": true
      }
   }
  }, 
  "coc.preferences.useQuickfixForLocations": true,
  "list.normalMappings": {
    "<C-c>": "do:exit",
    "<C-n>": "do:next",
    "<C-p>": "do:previous"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit",
    "<C-n>": "do:next",
    "<C-p>": "do:previous"
  }
}

