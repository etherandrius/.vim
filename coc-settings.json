{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
   }
  }, 
  "coc.preferences.useQuickfixForLocations": true,
  "list.normalMappings": {
    "<C-c>": "do:exit",
    "<C-n>": "do:next",
    "<C-p>": "do:previous"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit",
    "<C-n>": "do:next",
    "<C-p>": "do:previous"
  }
}
